<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.MainMenu.MainMenuController"
            prefHeight="400.0" prefWidth="600.0">
    <center>
                <VBox alignment="CENTER" spacing="10">
                    <HBox alignment="CENTER" spacing="10">
                        <Button text="start" onMouseClicked="#start"/>
                        <Button text="resume" onMouseClicked="#resume"/>
                    </HBox>
                    <HBox alignment="CENTER" spacing="10">
                        <Button text="profile" onMouseClicked="#profile"/>
                        <Button text="scoreboard" onMouseClicked="#scoreboard"/>
                    </HBox>
                    <HBox alignment="CENTER" spacing="10">
                        <Button text="logout" onMouseClicked="#logout"/>
                        <Button text="setting" onMouseClicked="#setting"/>
                    </HBox>
                </VBox>
   </center>

</BorderPane>
